{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "concurrently": "^4.1.0",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "express-favicon": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "serve": "^10.1.2",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.14.0",
    "typescript": "3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "tslint --fix",
        "git add"
      ],
      "*.{css,less,scss}": [
        "stylelint --fix",
        "git add"
      ]
    }
  },
  "proxy": "http://localhost:8000",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint": "stylelint \"**/*.css\" --fix",
    "tslint": "tslint client/src/**/*.{ts,tsx,js,jsx} --fix",
    "heroku-postbuild": "npm run build",
    "client-install": "npm install",
    "server": "sudo nodemon server.js",
    "client": "sudo npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
